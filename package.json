{
  "name": "server-main",
  "private": true,
  "scripts": {
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "pre-commit": "lint-staged -v",
    "dev": "next",
    "prebuild": "run-s generate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "format": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "migrate-dev": "dotenv -e .env.local prisma migrate dev --preview-feature --name init",
    "migrate-prd": "dotenv -e .env.local prisma migrate deploy --preview-feature --name init",
    "migrate-reset": "dotenv -e .env.local prisma migrate reset --preview-feature --name init",
    "seed": "dotenv -e .env.local prisma db seed --preview-feature",
    "generate": "run-s generate:prisma",
    "generate:prisma": "prisma generate",
    "init": "run-s init:env",
    "init:env": "cp .env.local.sample .env.local",
    "plop": "plop"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run test",
      "bash -c 'tsc --noEmit --project tsconfig.json'",
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@line/bot-sdk": "^7.5.2",
    "@line/liff": "^2.22.3",
    "@line/liff-mock": "^1.0.3",
    "@prisma/client": "^4.14.1",
    "@tailwindcss/forms": "^0.5.4",
    "aws-amplify": "^5.2.1",
    "aws-sdk": "^2.1359.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "firebase": "^10.3.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "^13.0.6",
    "next-connect": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.9.0",
    "react-swipeable": "^7.0.0",
    "recoil": "^0.7.7",
    "sharp": "^0.32.4",
    "swr": "^2.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@svgr/webpack": "^7.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "autoprefixer": "^10.4.13",
    "commitlint-plugin-scope-empty-exceptions": "^1.1.0",
    "commitlint-plugin-workspace-scopes": "^1.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "dotenv-cli": "^7.2.1",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jest": "^27.2.5",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "postcss": "^8.4.20",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.14.1",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.3"
  }
}
